{
  "name": "solshield",
  "version": "0.1.0",
  "description": "SolShield SDK - AI-Powered Smart Contract Security Scanner for Solana",
  "author": "SolShield Team",
  "license": "MIT",
  "homepage": "https://github.com/oh-ashen-one/solshield",
  "repository": {
    "type": "git",
    "url": "https://github.com/oh-ashen-one/solshield.git"
  },
  "bugs": {
    "url": "https://github.com/oh-ashen-one/solshield/issues"
  },
  "keywords": [
    "solana",
    "anchor",
    "smart-contract",
    "security",
    "audit",
    "vulnerability",
    "scanner",
    "sdk",
    "solshield"
  ],
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18"
  }
}
