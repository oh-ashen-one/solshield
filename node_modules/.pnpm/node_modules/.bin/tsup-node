#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Midir/clawd/projects/solguard/node_modules/.pnpm/tsup@8.5.1_jiti@2.6.1_postc_61be1d2ce04c10450065902d2329c6e3/node_modules/tsup/dist/node_modules:/mnt/c/Users/Midir/clawd/projects/solguard/node_modules/.pnpm/tsup@8.5.1_jiti@2.6.1_postc_61be1d2ce04c10450065902d2329c6e3/node_modules/tsup/node_modules:/mnt/c/Users/Midir/clawd/projects/solguard/node_modules/.pnpm/tsup@8.5.1_jiti@2.6.1_postc_61be1d2ce04c10450065902d2329c6e3/node_modules:/mnt/c/Users/Midir/clawd/projects/solguard/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Midir/clawd/projects/solguard/node_modules/.pnpm/tsup@8.5.1_jiti@2.6.1_postc_61be1d2ce04c10450065902d2329c6e3/node_modules/tsup/dist/node_modules:/mnt/c/Users/Midir/clawd/projects/solguard/node_modules/.pnpm/tsup@8.5.1_jiti@2.6.1_postc_61be1d2ce04c10450065902d2329c6e3/node_modules/tsup/node_modules:/mnt/c/Users/Midir/clawd/projects/solguard/node_modules/.pnpm/tsup@8.5.1_jiti@2.6.1_postc_61be1d2ce04c10450065902d2329c6e3/node_modules:/mnt/c/Users/Midir/clawd/projects/solguard/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-node.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-node.js" "$@"
fi
